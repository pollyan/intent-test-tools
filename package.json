{
  "name": "midscenejs-python-demo",
  "version": "1.0.0",
  "description": "Python + MidSceneJS Web UI自动化测试演示",
  "main": "index.js",
  "scripts": {
    "install-deps": "npm install",
    "test": "python -m pytest tests/ -v",
    "test:proxy": "jest tests/proxy --verbose",
    "test:proxy:watch": "jest tests/proxy --watch",
    "test:proxy:coverage": "jest tests/proxy --coverage",
    "test:proxy:ci": "jest tests/proxy --coverage --ci --watchAll=false --testResultsProcessor=jest-junit"
  },
  "dependencies": {
    "@midscene/web": "^0.22.1",
    "@playwright/test": "^1.45.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "playwright": "^1.45.0",
    "socket.io": "^4.7.0",
    "engine.io": "^6.5.0",
    "engine.io-client": "^6.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "superagent": "^8.1.2",
    "qs": "^6.11.0",
    "@jest/globals": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ws": "^8.14.2",
    "socket.io-client": "^4.7.4"
  },
  "keywords": [
    "midscene",
    "playwright",
    "automation",
    "testing",
    "ai"
  ],
  "author": "",
  "license": "MIT"
}
